var data = ['santai', 'berkualitas', 'I love Coding', ]

for (i = 1; i < 20; i++) {
    if (i % 2 == 0) {
        document.write(i + '.' + data[0])
    } if (i % 2 == 1) {
        document.write(i +  '.' + data[1])
    } if (i == 3 || i == 9 || i == 15 ) {
        document.write(i +  '.' + data[2])
    }
    document.write('<br>')
}
